<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="AndroidCommonAntiPatterns.com">

  <application
      android:name=".callback.MyApp"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".callbackwithannotation.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".callback.ActivityOne"
        android:label="@string/title_activity_first_activity"
        android:theme="@style/AppTheme.NoActionBar">
    </activity>
    <activity
        android:name=".baseactivity.SecondActivity"
        android:label="@string/title_activity_second_activity">
    </activity>
    <activity
        android:name=".baseactivity.FirstActivity"
        android:label="@string/title_activity_one">
    </activity>
    <activity
        android:name=".callback.ActivityTwo"
        android:label="@string/title_activity_two">
    </activity>
  </application>

</manifest>